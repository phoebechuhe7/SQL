CREATE TABLE celebrities (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    fullname TEXT,
    age INTEGER);
    
CREATE TABLE hobbies (
    id INTEGER PRIMARY KEY, 
    who TEXT,
    what TEXT); 
    
CREATE TABLE significant_others (
    id INTEGER PRIMARY KEY,
    who TEXT,
    partner TEXT); 
    
    
