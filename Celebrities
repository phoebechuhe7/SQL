SELECT fullname, hobbies.what FROM celebrities
    JOIN hobbies 
    ON celebrities.fullname = hobbies.who;

SELECT celebrities.fullname AS "Celebrity", significant_others.who AS "Significant Other" FROM celebrities
    JOIN significant_others 
    ON celebrities.fullname = significant_others.partner; 
    
    
